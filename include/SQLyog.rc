    IDM_EDIT_TEMP_DELETE    "/*\r\nDelete Statement\r\nSource : MySQL documentation\r\n*/\r\nDELETE [ LOW_PRIORITY ] FROM tbl_name\r\n\t[ WHERE where_definition ]\r\n\t[ LIMIT rows ]"
