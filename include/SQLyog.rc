    IDM_EDIT_TEMP_ALTER     "/*\r\nAlter Table Statement\r\nSource : MySQL documentation\r\n*/\r\nALTER [ IGNORE ] TABLE tbl_name alter_spec \r\n\t[ , alter_spec ... ]"
